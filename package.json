{
  "name": "support-drawer-redux",
  "version": "0.0.3",
  "description": "An inline support tool that allows you to embed support icons in your app that open up relevant documentation inside a drawer when clicked.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "jest src",
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build",
    "pre-commit": "lint-staged"
  },
  "pre-commit": [
    "pre-commit"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "support",
    "drawer",
    "icon",
    "tooltip",
    "react",
    "redux",
    "material"
  ],
  "author": "Benjamin W Stookey",
  "license": "MIT",
  "repository": "github:jamstooks/support-drawer-redux",
  "dependencies": {
    "@material-ui/core": "1.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-markdown": "^3.6.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "jest": "^23.2.0",
    "lint-staged": "^7.2.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.2"
  }
}
